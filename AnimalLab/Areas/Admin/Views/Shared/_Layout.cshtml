<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    <link href="~/Content/bootstrap-rtl.css" rel="stylesheet" />
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("حیوان", "Index", "Admin/Animals", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("نوع حیوان", "Index", "Admin/AnimalTypes", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("نوع تست", "Index", "Admin/TestTypes", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("اشخاص", "Index", "Admin/Person", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("نتیجه", "Index", "Admin/Result", new { area = "" }, new { @class = "navbar-brand" })

            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                </ul>
            </div>
        </div>
    </div>

    <div class="container body-content">

        @RenderBody()
        <hr />
    </div>

    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>